import discord.ext

client = discord.Client()

@client.event
async def on_ready():
    print("Le bot est prÃªt.")

@client.event
async def on_message(message):
    if message.content.lower() == "val":
        await message.channel.send("c moi wsh insta : https://www.instagram.com/lowsval/" , delete_after=5)






client.run("")





